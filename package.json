{
  "name": "health-poc",
  "version": "0.0.1",
  "main": "src/main.ts",
  "scripts": {
    "start-dev-db": "ts-node dev/db/start-dev-db.ts",
    "start-dev-http": "PORT=43000 ts-node-dev --respawn --transpile-only src/main.ts",
    "start-dev": "concurrently --kill-others --names \"DB,Node\" --prefix-colors \"blue,cyan\" \"npm run start-dev-db\" \"npm run start-dev-http\"",
    "encrypt-env": "ts-node dev/env/env-crypto.ts encrypt .env .env.encrypted",
    "decrypt-env": "ts-node dev/env/env-crypto.ts decrypt .env.encrypted .env"
  },
  "keywords": [],
  "author": "Greg Brooks <gb_sea@icloud.com>",
  "license": "UNLICENSED",
  "description": "POC system that integrations with EngagedMD external API",
  "dependencies": {
    "@nestjs/config": "^4.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/typeorm": "^11.0.0",
    "connect-pg-simple": "^10.0.0",
    "ejs": "^3.1.10",
    "express-session": "^1.18.2",
    "passport": "^0.7.0",
    "passport-auth0": "^1.4.5",
    "passport-session": "^1.0.2",
    "pg": "^8.16.3",
    "typeorm": "^0.3.27",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/express-session": "^1.18.2",
    "@types/node": "^24.10.1",
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
